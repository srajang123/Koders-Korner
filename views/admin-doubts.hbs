<div class="message doubtit">
    <div class="error">
        <h2>&nbsp;</h2>
    </div>
    <form action="/admin/doubt-mail" name="doubt-mail" method="post" onsubmit="return doubts()">
        <div class="doubts">
            <table class="doubts">
                <tr class="doubt-field">
                    <td class="doubt-field">Name</td>
                    <td class="doubt-field right"><input type="text" class="text" name="name" id="name" disabled></td>
                </tr>
                <tr class="doubt-field">
                    <td class="doubt-field">E-Mail</td>
                    <td class="doubt-field right"><input type="text" class="text" name="email" id="email" disabled></td>
                </tr>
